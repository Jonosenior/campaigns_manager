<div class="container">
  <div class="col-lg-5 col-lg-offset-3">

    <h1><%= @todo_list.title %></h1>
    <p>For: <%= @todo_list.campaign.title %> </p>
    <u>Todos</u>
      <ul><% @todo_list.todos.each do |t| %>
        <li><%= t.title %></li>
          <% end %>
      </ul>
    <%= render partial: 'comments/comments', locals: { commentable: @todo_list } %>
    <%= render partial: 'comments/form', locals: { commentable: @todo_list } %>
  </div>
</div>
